#include <stdio.h>
int main()
{
    int x;
    printf("Enter x.");
    scanf("%d",&x);
    printf("Initial value of x: %d\n", x);
    printf("x++ = %d\n", x++);  
    printf("After post-increment, x = %d\n", x);
    printf("++x = %d\n", ++x);  
    printf("After pre-increment, x = %d\n", x);
    return 0;
}
